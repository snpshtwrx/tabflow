{
	"manifest_version": 3,
	"name": "TabFlow",
	"version": "0.1",

	"permissions": ["tabs", "nativeMessaging"],

	"browser_specific_settings": {
		"gecko": {
			"id": "@tabflow",
			"strict_min_version": "109.0"
		}
	},
	
	"background": {
		"scripts": ["tabwatcher.js"]
	},

	"icons": {
		"128": "icons/tabflow.png"
	}
}
